<svg width="400" height="300" viewBox="0 0 400 300" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="festivalBg" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#FFF3E0;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#FFE0B2;stop-opacity:1" />
    </linearGradient>
  </defs>
  <rect width="400" height="300" fill="url(#festivalBg)"/>
  
  <!-- Traditional Chinese elements -->
  <!-- Lanterns -->
  <ellipse cx="80" cy="80" rx="25" ry="35" fill="#E53935" stroke="#C62828" stroke-width="2"/>
  <rect x="75" y="45" width="10" height="15" fill="#8D6E63"/>
  <text x="80" y="85" font-family="serif" font-size="14" text-anchor="middle" fill="#FFD700">福</text>
  
  <ellipse cx="320" cy="70" rx="20" ry="30" fill="#FFC107" stroke="#FF8F00" stroke-width="2"/>
  <rect x="317" y="40" width="6" height="12" fill="#8D6E63"/>
  <text x="320" y="75" font-family="serif" font-size="12" text-anchor="middle" fill="#D32F2F">春</text>
  
  <!-- Stage with performers -->
  <rect x="100" y="180" width="200" height="100" fill="#8D6E63" rx="5"/>
  <rect x="110" y="170" width="180" height="20" fill="#A1887F" rx="3"/>
  
  <!-- Dancers -->
  <ellipse cx="150" cy="220" rx="12" ry="20" fill="#E91E63"/>
  <circle cx="150" cy="205" r="6" fill="#F8BBD9"/>
  <rect x="145" y="200" width="10" height="8" fill="#000" rx="2"/>
  
  <ellipse cx="200" cy="220" rx="12" ry="20" fill="#9C27B0"/>
  <circle cx="200" cy="205" r="6" fill="#F8BBD9"/>
  <rect x="195" y="200" width="10" height="8" fill="#000" rx="2"/>
  
  <ellipse cx="250" cy="220" rx="12" ry="20" fill="#3F51B5"/>
  <circle cx="250" cy="205" r="6" fill="#F8BBD9"/>
  <rect x="245" y="200" width="10" height="8" fill="#000" rx="2"/>
  
  <!-- Musical notes -->
  <circle cx="130" cy="120" r="4" fill="#2196F3"/>
  <rect x="134" y="110" width="2" height="20" fill="#2196F3"/>
  
  <circle cx="270" cy="130" r="4" fill="#4CAF50"/>
  <rect x="274" y="120" width="2" height="20" fill="#4CAF50"/>
  
  <!-- Calligraphy brush -->
  <rect x="50" y="150" width="4" height="40" fill="#8D6E63"/>
  <ellipse cx="52" cy="145" rx="3" ry="8" fill="#424242"/>
  
  <!-- Chinese characters -->
  <text x="200" y="50" font-family="serif" font-size="24" text-anchor="middle" fill="#D32F2F" font-weight="bold">中华文化节</text>
  
  <!-- Decorative clouds -->
  <ellipse cx="60" cy="40" rx="15" ry="8" fill="white" opacity="0.7"/>
  <ellipse cx="340" cy="35" rx="12" ry="6" fill="white" opacity="0.7"/>
</svg>